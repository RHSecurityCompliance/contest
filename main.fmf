require:
  - scap-security-guide

# shared by all tests
recommend:
  - python3
  - python36
  - python3-requests
  - python36-requests
  - python3-rpm
  - python36-rpm
  # these are needed for CONTEST_CONTENT or get_content():
  # - builddep on scap-security-guide.spec
  - python-srpm-macros
  # - preparing/patching downloaded SRPM
  - rpm-build
component:
  - scap-security-guide

# don't test for SELinux AVCs in Beaker
environment:
    AVC_ERROR: +no_avc_check
